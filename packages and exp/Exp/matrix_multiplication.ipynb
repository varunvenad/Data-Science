    "#matrix multiplication\n",
    "import numpy as np\n",
    "\n",
    "arr=np.arange(4).reshape(2,2)\n",
    "print(\"Matrix A:\\n\",arr)\n",
    "arr2=np.arange(5,9).reshape(2,2)\n",
